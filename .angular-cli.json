{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "game-jam2018"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        { "glob": "package.json", "input": "../", "output": "./assets/" },
        { "glob": "data.json", "input": "./", "output": "./assets/" },
        { "glob": "waiting.gif", "input": "./", "output": "./assets/" },
        { "glob": "splash.gif", "input": "./", "output": "./assets/" },
        { "glob": "Adutering.png", "input": "./", "output": "./assets/" },
        { "glob": "NANI1.png", "input": "./", "output": "./assets/" },
        { "glob": "OniiChan.png", "input": "./", "output": "./assets/" },
        { "glob": "REEEEE.png", "input": "./", "output": "./assets/" },
        { "glob": "WRYYYYYY.png", "input": "./", "output": "./assets/" },
        { "glob": "CHINA.png", "input": "./", "output": "./assets/" },
        { "glob": "Adutering.png", "input": "./", "output": "./assets/" },
        { "glob": "player1.jpg", "input": "./", "output": "./assets/" },
        { "glob": "player2.jpg", "input": "./", "output": "./assets/" },
        { "glob": "player3.jpg", "input": "./", "output": "./assets/" },
        { "glob": "player4.jpg", "input": "./", "output": "./assets/" }
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
